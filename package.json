{
  "name": "integrated-life",
  "version": "1.0.0",
  "description": "Integrated Life",
  "main": "app.js",
  "repository": "https://github.com/render-examples/express-hello-world",
  "author": "Sheridan Gray",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node app.js",
    "watch:sass": "sass ./public/css/style.scss public/css/style.css",
    "watch:js": "webpack --watch --mode development",
    "watch:all": "concurrently \"npm run watch:sass\" \"npm run watch:js\"",
    "start:dev": "npm run watch:all & nodemon app.js",
    "build:js": "webpack --mode production"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "material-icons": "^1.13.12",
    "mongoose": "^7.5.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "nodemon": "^3.0.1",
    "pug": "^3.0.2",
    "sass": "^1.68.0",
    "validator": "^13.11.0",
    "yarn": "^1.22.22"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^3.0.0",
    "concurrently": "^8.2.1",
    "prettier": "^3.0.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
