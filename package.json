{
  "name": "integrated-life",
  "version": "1.0.0",
  "description": "Integrated Life",
  "main": "app.js",
  "repository": "https://github.com/render-examples/express-hello-world",
  "author": "Sheridan Gray",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node app.js",
    "start:dev": "concurrently \"nodemon --watch 'public/js/**/*.js' --watch 'public/css/**/*.scss' --watch 'views/**/*' --watch 'routes/**/*.js' --watch 'models/**/*.js' --watch 'controllers/**/*.js' --watch 'app.js' --ext js,scss,ejs\" \"npm run watch:sass\" \"npm run watch:js\"",
    "watch:js": "webpack --watch --mode development",
    "watch:sass": "sass --watch public/css/style.scss:public/css/dist/style.css --source-map",
    "watch:all": "concurrently \"npm run watch:sass\" \"npm run watch:js\"",
    "build:js": "webpack --mode production",
    "build:css": "sass public/css/style.scss:public/css/dist/style.css --style=compressed --no-source-map",
    "build": "npm run build:js && npm run build:css",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.726.1",
    "@aws-sdk/lib-storage": "^3.726.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csurf": "^1.10.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "google-auth-library": "^9.15.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "marked": "^15.0.3",
    "material-icons": "^1.13.12",
    "mongoose": "^7.5.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "nodemon": "^3.0.1",
    "pug": "^3.0.2",
    "slugify": "^1.6.6",
    "together-ai": "^0.9.0",
    "validator": "^13.11.0",
    "yarn": "^1.22.22"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@prettier/plugin-pug": "^3.0.0",
    "babel-loader": "^9.2.1",
    "concurrently": "^8.2.2",
    "prettier": "^3.0.3",
    "sass": "^1.83.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4"
  }
}
